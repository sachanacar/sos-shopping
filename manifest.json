{
  "manifest_version": 2,

  "name": "SOS Shopping",
  "description": "Get in touch with an agent that can help you make proper shopping decisions!",
  "version": "1.0",
  "icons": {
      "128": "icon.png"
    },
  "browser_action": {
   "default_icon": "icon.png",
   "default_title": "SOS Shopping"
  },
  "background": {
  "scripts": ["./js/popup.js"],
  "persistent": false
  },
  "permissions": [
   "activeTab"
   ]
}